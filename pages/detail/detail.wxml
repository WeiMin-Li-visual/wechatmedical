<!--pages/detail/detail.wxml-->

<view class="main">
  <view class="topthumb">
    <image src="{{healthknos.thumb}}" mode="scaleToFill"></image>
  </view>

  <view class="inner">
    <view class="utitle">
      <view class="liy"></view><text>{{healthknos.name}}</text>
    </view>

    <view class="udetail">
      <view wx:for="{{healthknos.detail}}" wx:key="id">
        {{healthknos.detail[index]}}
      </view>
    </view>
  </view>
</view>
